                            <div class="div-type-1" ng-switch-when="2" title="@Html.Raw(Succ.Common.Global.Succ.Pressing)">&nbsp;</div>
                            <div class="div-type-2" ng-switch-when="1" title="@Html.Raw(Succ.Common.Global.Succ.Important)">&nbsp;</div>
