
@{
    Layout = null; 
}

<form ng-submit="emailsubmit()" ng-controller="forgetpassword">
    <div class="main">
        <div class="dialog">

            <div id="see_id" style="font-size:18px">@Html.Raw(Succ.Common.Global.Succ.ForgotPpassword)?</div>
            <div style="margin-top:30px;">
                <input type="text" id="txtEmail" class="email" ng-model="txtEmail" /><br />
                <input type="submit" value="@Html.Raw(Succ.Common.Global.Succ.OK)" class="submit_button" id="logb" />
                <h2>{{list}}</h2>
            </div>
        </div>
    </div>
</form>

