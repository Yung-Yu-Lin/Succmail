@{
    Layout = null;
    @model Succ.Common.Models.Login.LoginState_Info
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no" />
    <meta http-equiv="pragma" content="no-cache" />
    <title>Succmail</title>
    <link href="~/Statics/css/home/LoginPage.min.css" rel="stylesheet" />
    <link href="~/Statics/css/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <script src="/Scripts/lib/jquery/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/lib/bootstrap/bootstrap.min.js"></script>
    <link href="/Statics/css/home/animate.css" rel="stylesheet" />
    <!-- #region 不能包入ScrollAnimate，所以不使用Bundle -->
    <script src="~/Scripts/common/home/HomeLogic.js?t=1"></script>
    <script src="~/Scripts/common/FullwidthConvert.js"></script>
    <script src="~/Scripts/lib/jquery-ui/jquery.blockUI.js"></script>
    <script src="~/Scripts/common/home/about.js"></script>
    <!-- #endregion -->
    <link href="~/Statics/css/home/ScrollStyle.css" rel="stylesheet" />
</head>
<body>
    @*<iframe width="560" height="315" src="https://www.youtube.com/embed/fP6sriWrX5k" frameborder="0" allowfullscreen=allowfullscreen></iframe>*@
    <!-- #region 隱藏參數 Subjid、isEmail -->
    <input id="subjectID" type="hidden" value="@Model.SubjectID" />
    <input id="isEmail" type="hidden" value="@Model.isEmail" />
    <!-- #endregion -->
    <!-- #region Youtube -->
    <div id="playYoutube-ground">
        <div class="closeLogin_button"></div>
    </div>
    <!-- #endregion -->
    <!-- #region 主板範圍 -->
    <div id="login-ground">
        <div class="closeLogin_button"></div>
        <!-- #region 登入畫面 -->
        <div id="login-table" style="display:none;">
            <div id="login-title">
                <img id="logo_white" />
            </div>
            <div id="ac-pw-div">
                <input style="display:none">
                <input type="password" style="display:none">
                <input id="account" type="tel" placeholder="E-mail" onkeyup="Validate(this.value, this)" style="-ms-ime-mode:disabled;ime-mode:disabled;" autocomplete="off" onfocus="FocusAcc()" />
                <input id="password" type="password" placeholder="Password" style="-ms-ime-mode:disabled;ime-mode:disabled;" autocomplete="off" />
                <a id="forgetPW" href="/Home/Forget">@Html.Raw(Succ.Common.Global.Succ.ForgotPpassword)?</a>
                <input id="login-button" type="submit" value="@Html.Raw(Succ.Common.Global.Succ.Login)" onclick="Login()" />
            </div>
        </div>
        <!-- #endregion -->
    </div>
    <!-- #endregion -->
    <!-- #region 最上方Nav Bar -->
    <nav class="navbar navbar-default padding10" role="navigation">
        <!-- #region Logo -->
        <div class="navbar-brand" id="logo"></div>
        <!-- #endregion -->
        <div class="container">
            <div id="mainNav" class="navbar-header mainNavLast">
                <!-- #region 畫面縮小列表按鈕 -->
                <button id="LoginNavBtn" type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#loginPage" aria-expanded="false">
                    <span class="sr-only">Toggle</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <!-- #endregion -->
                <!-- #region 登入按鈕 -->
                <a id="openLogin" class="navbar-brand" title="@Html.Raw(Succ.Common.Global.Succ.Login)">
                    @Html.Raw(Succ.Common.Global.Succ.Login)
                </a>
                <!-- #endregion -->
            </div>
            <!-- #region 下拉選項列表 -->
            <div class="collapse navbar-collapse" id="loginPage" aria-expanded="true">
                <ul class="nav navbar-nav loginNav">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" title="@Html.Raw(Succ.Common.Global.Succ.Service)">
                            @Html.Raw(Succ.Common.Global.Succ.Price_And_Service)
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu" style="width:100%;">
                            <li>
                                <a href="/Home/Login_pri_serv_contact">@Html.Raw(Succ.Common.Global.Succ.Contact)Succmail</a>
                            </li>
                            <li>
                                <a href="/Home/Login_Network_Security">@Html.Raw(Succ.Common.Global.Succ.Net_Safe)</a>
                            </li>
                            <li>
                                <a href="/Home/Login_ClientCase">@Html.Raw(Succ.Common.Global.Succ.Cus_Sample)</a>
                            </li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" title="@Html.Raw(Succ.Common.Global.Succ.About)Succmail">
                            @Html.Raw(Succ.Common.Global.Succ.About)Succmail
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu" style="width:100%;">
                            <li><a href="/Home/Login_about_what">@Html.Raw(Succ.Common.Global.Succ.What_is)Succmail</a></li>
                            <li><a href="/Home/Login_about_why">@Html.Raw(Succ.Common.Global.Succ.Why)Succmail</a></li>
                            <li><a href="/Home/Login_about_who">@Html.Raw(Succ.Common.Global.Succ.Who)Succmail</a></li>
                            <li><a href="/Home/Login_about_where">@Html.Raw(Succ.Common.Global.Succ.AnyWhere)</a></li>
                            <li><a href="/Home/Login_about_how">@Html.Raw(Succ.Common.Global.Succ.How)Succmail</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
            <!-- #endregion -->
        </div>
    </nav>
    <!-- #endregion -->
    <div id="ground-wrap">
        <!-- #region 註冊區塊 -->
        <div id="main-top-part-index">
            <div id="main-container">
                <div id="wrap-slogan-register">
                    <img id="slogan-index" class="animated fadeIn" src="/Statics/img/home/slogan_index.png" />
                    <div id="register">
                        @using (Html.BeginForm("Regist", "User", FormMethod.Post, new { id = "emailvalue" }))
                        {
                            <input style="display:none">
                            <input type="password" style="display:none">
                            <input type="text" name="email" id="email2" class="type-regis animated fadeIn" placeholder="Enter your work email" onkeyup="Validate(this.value, this)" style="-ms-ime-mode:disabled;ime-mode:disabled;" autocomplete="off" />
                            <input id="logb" type="submit" class="button-regis animated fadeIn" value="@Html.Raw(Succ.Common.Global.Succ.Sign_up)" onclick="onSubmit()" />
                        }
                    </div>
                </div>
            </div>
        </div>
        <!-- #endregion -->
        <!-- #region BMCA合作照片 -->
        <div id="bottom-register-wrap" class="animation-element slide-left">
            <a href="http://www.bmca.org.tw/" target="_blank">
                <img id="bmcaImg" src="/Statics/img/home/cooperation.png" />
            </a>
        </div>
        <!-- #endregion -->
        <!-- #region 4w1h -->
        <div id="wh-menu-wrap">
            <a class="menu" href="/Home/Login_about_what">What</a><span class="separate">&nbsp;/&nbsp;</span>
            <a class="menu" href="/Home/Login_about_why">Why</a><span class="separate">&nbsp;&nbsp;/&nbsp;&nbsp;</span>
            <a class="menu" href="/Home/Login_about_who">Who</a><span class="separate">&nbsp;&nbsp;/&nbsp;&nbsp;</span>
            <a class="menu" href="/Home/Login_about_where">Where</a><span class="separate">&nbsp;&nbsp;/&nbsp;&nbsp;</span>
            <a class="menu" href="/Home/Login_about_how">How</a>
        </div>
        <!-- #endregion -->
        <!-- #region 使用者推薦 -->
        <div id="introduce-wrap" class="container">
            <div class="row">
                <div class="feedback-wrap col-sm-6">
                    <h3 class="title">@Html.Raw(Succ.Common.Global.Succ.Doctor_Jiang)</h3>
                    <p class="text">@Html.Raw(Succ.Common.Global.Succ.Doctor_Jiang_Ex)</p>
                </div>
                <div class="feedback-wrap col-sm-6">
                    <h3 class="title">@Html.Raw(Succ.Common.Global.Succ.Chen_Job)<br />@Html.Raw(Succ.Common.Global.Succ.Mr_Chen)</h3>
                    <p class="text">@Html.Raw(Succ.Common.Global.Succ.Mr_Chen_Ex)</p>
                </div>
            </div>
        </div>
        <!-- #endregion -->
        <!-- #region youtube區塊 -->
        <div id="youtube-wrap">
            <div style="width:70%; margin:0px 15%; display:inline-block; cursor:pointer;">
                <img id="  " width="100%" src="/Statics/img/home/systemScreen.png" />
            </div>
        </div>
        <!-- #endregion -->
        <!-- #region 底部Footer -->
        <div id="footer-wrap" class="container">
            <div class="row">
                <div class="col-sm-6 footerInfo">
                    <span>@Html.Raw(Succ.Common.Global.Succ.Cus_Service)</span>
                    <section class="serviceEmail">
                        <img />
                    </section>
                    <section class="servicetel">
                        <img />
                    </section>
                    <section class="serviceaddress">
                        <img />
                    </section>
                </div>
                <div class="col-sm-6 footerInfo">
                    <span>@Html.Raw(Succ.Common.Global.Succ.Promotion_Product)</span>
                    <section class="BMCAEmail">
                        <img />
                    </section>
                    <section class="BMCAtel">
                        <img />
                    </section>
                    <section class="BMCAaddress">
                        <img />
                    </section>
                </div>
            </div>
        </div>
        <!-- #endregion -->
    </div>
    <script>

    </script>
</body>
</html>